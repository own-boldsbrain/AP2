{
      "name": "AneelKpisAgent",
      "description": "An agent for calculating and retrieving ANEEL electricity KPIs.",
      "capabilities": {
            "extensions": [
                  {
                        "uri": "https://github.com/google-agentic-commerce/ap2/v1",
                        "description": "Supports the Agent Payments Protocol.",
                        "required": true
                  }
            ]
      },
      "skills": [
            {
                  "id": "calculate_electricity_kpis",
                  "name": "Calculate Electricity KPIs",
                  "description": "Calculates key performance indicators for electricity consumption and generation.",
                  "parameters": {
                        "type": "object",
                        "properties": {
                              "consumption_data": {
                                    "type": "array",
                                    "description": "Array of consumption values."
                              },
                              "generation_data": {
                                    "type": "array",
                                    "description": "Array of generation values."
                              }
                        },
                        "required": [
                              "consumption_data",
                              "generation_data"
                        ]
                  },
                  "tags": [
                        "aneel",
                        "kpis",
                        "electricity"
                  ]
            },
            {
                  "id": "get_kpi_report",
                  "name": "Get KPI Report",
                  "description": "Generates a report of ANEEL KPIs for a given period.",
                  "parameters": {
                        "type": "object",
                        "properties": {
                              "start_date": {
                                    "type": "string",
                                    "description": "Start date for the report."
                              },
                              "end_date": {
                                    "type": "string",
                                    "description": "End date for the report."
                              }
                        },
                        "required": [
                              "start_date",
                              "end_date"
                        ]
                  },
                  "tags": [
                        "aneel",
                        "kpis",
                        "report"
                  ]
            }
      ],
      "defaultInputModes": [
            "text/plain"
      ],
      "defaultOutputModes": [
            "application/json"
      ],
      "url": "http://localhost:8005/mcp/aneel_kpis",
      "version": "1.0.0"
}
