{
  "id": "aneel.kpis.agent",
  "name": "ANEEL KPIs Agent",
  "type": "data-provider",
  "version": "0.1.0",
  "runtime": {
    "type": "python",
    "entrypoint": "domains/origination-viabilidade/apps/aneel_kpis/app/main.py",
    "requires": ["fastapi>=0.115","uvicorn>=0.30","pandas","requests","pydantic>=2.7"]
  },
  "tools": [
    { "name": "aneel.kpis.indger.fetch", "summary": "Indicadores gerenciais da distribuição.", "input_schema": { "type":"object","required":["distribuidora"], "properties": { "distribuidora":{"type":"string"}, "mes":{"type":"string","pattern":"^[0-9]{4}-[0-9]{2}$"}, "tipo":{"type":"string","enum":["comercial","tecnico","subestacoes","alimentadores"],"default":"comercial"} } }, "output_schema": { "type":"object","required":["kpis"], "properties": { "kpis":{"type":"array","items":{"type":"object"}}, "source":{"type":"string"} } } },
    { "name": "aneel.kpis.iasc.fetch",  "summary": "Índice de satisfação do consumidor.", "input_schema": { "type":"object","required":["ano"], "properties": { "ano":{"type":"integer"}, "distribuidora":{"type":"string"} } }, "output_schema": { "type":"object","required":["iasc"], "properties": { "iasc":{"type":"array","items":{"type":"object"}}, "source":{"type":"string"} } } },
    { "name": "aneel.load_curve.ctr.fetch", "summary": "Curvas de carga (consumidor/rede).", "input_schema": { "type":"object","required":["tipo"], "properties": { "tipo":{"type":"string","enum":["consumidor_tipo","rede_tipo"]}, "uf":{"type":"string"}, "distribuidora":{"type":"string"} } }, "output_schema": { "type":"object","required":["curves"], "properties": { "curves":{"type":"array","items":{"type":"object"}}, "source":{"type":"string"} } } }
  ],
  "security": { "pii": false, "allowed_network": ["https://dadosabertos.aneel.gov.br/*"] }
}
