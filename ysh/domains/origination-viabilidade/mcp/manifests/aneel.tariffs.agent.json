{
  "id": "aneel.tariffs.agent",
  "name": "ANEEL Tariffs Agent",
  "type": "data-provider",
  "version": "0.1.1",
  "runtime": {
    "type": "python",
    "entrypoint": "domains/origination-viabilidade/apps/aneel_tariffs/app/main.py",
    "requires": ["fastapi>=0.115","uvicorn>=0.30","pandas","requests","nats-py>=2.6","pydantic>=2.7"]
  },
  "tools": [
    {
      "name": "aneel.tariffs.components.fetch",
      "summary": "Busca componentes TE/TUSD por vigÃªncia/subgrupo/classe.",
      "input_schema": { "type":"object","required":["sig_agente","inicio_vigencia"], "properties": { "sig_agente":{"type":"string"}, "inicio_vigencia":{"type":"string","format":"date"}, "fim_vigencia":{"type":"string","format":"date"}, "subgrupo":{"type":"string"}, "modalidade":{"type":"string"}, "classe":{"type":"string"}, "posto_tarifario":{"type":"string"}, "cnpj":{"type":"string"} } },
      "output_schema": { "type":"object","required":["rows"], "properties": { "rows":{"type":"array","items":{"type":"object"}}, "source":{"type":"string"} } }
    },
    {
      "name": "aneel.tariffs.profile.build",
      "summary": "Agrega componentes em tariff_profile.",
      "input_schema": { "type":"object","required":["rows"], "properties": { "rows":{"type":"array","items":{"type":"object"}} } },
      "output_schema": { "type":"object","required":["tariff_profile"], "properties": { "tariff_profile":{"type":"object"} } }
    }
  ],
  "security": { "pii": false, "allowed_network": ["https://dadosabertos.aneel.gov.br/*"] }
}
